<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<UI size="1100,700">
	<Control Class="CLabelUI" DesignerClass="CLabelUI" position="0,0,100,100" Name="title" FixedOnView="true" Layout="fill,40" back-show="always" text-align="hcenter|vcenter" text="tr:弹出抽象窗体" labeltype="label"/>

	<hLayout Layout="fill,400">
		<Control Class="CContainLayoutUI" Name="vWinds" contain-flags="GroupLineStyle" text="tr:CContainerUI 控件容器"  position="350,80, 300, 400" border-show="true" back-gradient="normal:conic_blue; highlight:radial_blue; selected:bru_orange_red_dwn; disable:bru_orange_red_dis">
			<vlayout Layout="fill,fill" interval='3'>
				<Hlayout Layout="fill,fill" interval='3'>
					<Control Class="CLabelUI" Layout="adapt,fill" Font="font-level1" text="tr:Position" text-align="right"  />
					<Control Class="CEditTextUI" Name="Latitude" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="Longitude" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="Elevation" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="Nominal" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="Pressure" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="Gradient" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="TransferHeight" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
				</Hlayout>
				<Hlayout Layout="fill,fill" interval='3'>
					<Control Class="CLabelUI" Layout="adapt,fill" Font="font-level1" text="tr:System" text-align="right"  />
					<Control Class="CEditTextUI" Name="InstrumentType" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="SNNumber" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="LaserType" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CLabelUI" Layout="adapt,fill" Font="font-level1" text="tr:Acquisition" text-align="right"  />

					<Control Class="CEditTextUI" Name="SetCount" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="DropCount" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="SetInterval" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
					<Control Class="CEditTextUI" Name="DropInterval" Layout="fill,fill"  border-show="always"  input-type="Decimal"  />
				</Hlayout>
			</vlayout>
		</Control>
	</hLayout>

	<Control Class="CLayoutArrowContainUI" Layout="300,200" input-range="0,100000" back-show="always"/>

</UI>